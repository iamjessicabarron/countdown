{
    "name": "Countdown",
    "version": "1.0",
    "description": "Countdown to events!",
    "permissions": [
      "storage",
      "activeTab"
    ],
    "background": {
      "scripts": [
        "lib/background.js"
      ],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Countdown", 
      "default_popup": "dist/popup.html"
    },
    "manifest_version": 2
  }