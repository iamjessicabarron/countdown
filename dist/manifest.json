{
    "name": "Countdown",
    "version": "0.0.0.1",
    "description": "A tiny Chrome Extension that tells you when stuff is, relative to right now.",
    "icons": {
      "128": "img/logo.png"
    },
    "permissions": [
      "storage"
    ],
    "background": {
      "scripts": [
        "js/background-min.js"
      ],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Countdown", 
      "default_popup": "popup.html"
    },
    "manifest_version": 2
  }