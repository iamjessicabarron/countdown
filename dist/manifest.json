{
    "name": "Countdown",
    "version": "0.0.0.10",
    "description": "A tiny Chrome Extension that tells you when stuff is, relative to right now.",
    "icons": {
      "16": "img/logo16.png",
      "60": "img/logo60.png",
      "48": "img/logo48.png",
      "128": "img/logo128.png"
    },
    "permissions": [
      "storage"
    ],
    "background": {
      "scripts": [
        "js/background-min.js"
      ],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Countdown", 
      "default_popup": "popup.html"
    },
    "manifest_version": 2
  }